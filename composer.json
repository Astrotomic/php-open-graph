{
    "name": "astrotomic/php-open-graph",
    "description": "Easily generate Open Graph tags",
    "keywords": [
        "open-graph"
    ],
    "homepage": "https://github.com/Astrotomic/php-open-graph",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://astrotomic.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "astrotomic/php-conditional-proxy": "^0.2.0"
    },
    "require-dev": {
        "pestphp/pest": "^0.3.0",
        "spatie/pest-plugin-snapshots": "^0.3.0"
    },
    "suggest": {
        "php": "^7.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\OpenGraph\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
